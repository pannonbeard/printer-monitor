    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.4.1",
    "nodemon": "^2.0.2",
    "wait-on": "^4.0.1"
  }
}
